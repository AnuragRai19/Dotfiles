#SHELL
shell /usr/bin/zsh

# MODIFIER KEY
kitty_mod ctrl+shift


# BEGIN_KITTY_THEME
# Dracula
include current-theme.conf
# END_KITTY_THEME

#FONTS
font_family FiraMono Nerd Font
bold_font   FiraMono Nerd Font Bold
italic_font FiraMono Nerd Font Italic
bold_italic_font  FiraMono Nerd Font Bold Italic

font_size 12

# WINDOWS
window_margin_width 5
window_border_width 2pt

# TABS
tab_bar_style powerline


